{
    "name": "YouTube Video Speed Doubler",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Doubles the playback speed of YouTube videos.",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "permissions": [
      "activeTab"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_title": "Double Playback Speed",
      "default_icon": "icon48.png"
    }
  }
  